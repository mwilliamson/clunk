        @org.junit.jupiter.api.DisplayName("elements with non-empty children are not stripped")
        public void elementsWithNonEmptyChildrenAreNotStripped() {
            var nodes = java.util.List.of(Html.element("p", java.util.List.of(), java.util.List.of(Html.text("H"))));

            var result = Html.stripEmpty(nodes);

            assertThat(result, equalTo(java.util.List.of(Html.element("p", java.util.List.of(), java.util.List.of(Html.text("H"))))));
        }

